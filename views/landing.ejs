<style>
    /* Base styles for the transparent section */
    
    .transparent-section {
        position: relative;
        height: 30vh;
        background-image: url('/images/full/logo.jpg');
        background-attachment: fixed;
        background-size: cover;
        background-position: center;
        background-repeat: no-repeat;
    }
    
    .transparent-section::before {
        content: "";
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-color: rgba(53, 52, 110, 0.4);
        /* Dark overlay */
        z-index: 1;
        /* Overlay above background image */
    }
    
    .overlay {
        position: relative;
        z-index: 2;
        /* Text above overlay */
        display: flex;
        justify-content: center;
        align-items: center;
        height: 100%;
        text-align: center;
    }
    
    .overlay h1 {
        font-size: 4rem;
        /* Adjust size as needed */
        font-weight: bold;
        /* Bold text */
        color: white;
        /* Text color */
        margin: 0;
        /* Remove default margin */
        padding: 0;
        /* Remove default padding */
    }
    /* Media queries for mobile view */
    
    @media (max-width: 768px) {
        .transparent-section {
            height: 20vh;
            /* Adjust height for smaller screens */
        }
        .overlay h1 {
            font-size: 2.5rem;
            /* Adjust font size for smaller screens */
        }
    }
    
    @media (max-width: 480px) {
        .transparent-section {
            height: 15vh;
            /* Further adjust height for very small screens */
        }
        .overlay h1 {
            font-size: 1.5rem;
            /* Further adjust font size for very small screens */
        }
    }
</style>

